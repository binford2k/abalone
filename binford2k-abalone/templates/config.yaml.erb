---
:port: <%= @port %>
:bind: <%= @bind %>
:logfile: <%= @logfile %>
<% if @ssh_host -%>
:ssh:
  :host: <%= @ssh_host %>
<% if @ssh_user -%>
  :user: <%= @ssh_user %>
<% end -%>
<% if @ssh_cert -%>
  :cert: <%= @ssh_cert %>
<% end -%>
<% if @ssh_port -%>
  :port:
<% end -%>
<% end -%>
<% if @autoconnect -%>
:autoconnect: <%= @autoconnect %>
<% end -%>
<% if @ttl -%>
:ttl: <%= @ttl %>
<% end -%>
<% if @timeout -%>
:timeout: <%= @timeout %>
<% end -%>
<% if @bannerfile -%>
:bannerfile: <%= @bannerfile %>
<% end -%>
<% if @welcome -%>
:welcome: <%= @welcome %>
<% end -%>
<% if @command -%>
:command: <%= @command %>
<% end -%>
<% if @params -%>
:params:
<% if @params.class == Hash -%>
  <% @params.keys.sort.each do |key, value| -%>
  <%= key %>:
  <% if value.class == Hash -%>
    <% if value.has_key? :map %>:map: <%= value[:map] %><% end -%>
    <% if value.has_key? :values %>:map: <%= value[:values].inspect %><% end -%>
  <% end -%>
  <% end -%>
<% else -%>
  <% @params.each do |value| -%>
    <%= value %>:
  <% end -%>
<% end -%>
<% end -%>
